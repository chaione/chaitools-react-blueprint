    "react-router-dom": "^4.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.64",
    "@types/node": "^10.12.7",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router-dom": "^4.0.4",
