  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config configs/webpack/dev.js --progress --colors",
    "build": "webpack --config configs/webpack/dev.js",
    "build:staging": "NODE_ENV=stagin webpack --config configs/webpack/staging.js",
    "build:production": "NODE_ENV=production webpack --config configs/webpack/production.js",
    "docker:build": "docker build -t chaione/project:tag --config config/webpack/Dockerfile .",
    "docker:build:production": "docker build -t chaione/project:tag --config config/webpack/Dockerfile.production .",
    "test": "jest"
  },
  "dependencies": {
    "@types/react-redux": "^4.4.40",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-persist": "^4.8.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.22",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router-dom": "^4.0.4",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "css-loader": "^0.28.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.5",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
