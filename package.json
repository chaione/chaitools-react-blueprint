    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
